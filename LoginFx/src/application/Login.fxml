            <Font size="18.0" />
         </font>
      </PasswordField>
      <TextField fx:id="txtUserName" layoutX="99.0" layoutY="172.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label fx:id="lblStatus" layoutX="15.0" layoutY="100.0" prefHeight="27.0" prefWidth="369.0" text="Status" textFill="#e80e0b">
